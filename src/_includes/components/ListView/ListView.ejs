<%
if(typeof props === 'undefined') {
  const props = {
    cardNum: 12,
    colNum: 3, // _c-list-view.scss依存 1~10の数字
    ulClassNames: "string",
  }
}
-%>
<div class="c-list-view">
  <div class="container">
    <ul class="row row-cols-<%= props.colNum%> <%= props.ulClassNames || null %>">
      <% for(let i = 0; i< props.cardNum; i++) { %>
      <li class="col">
        <div class="card">
          <div>
            <img src="https://placehold.jp/100x100.png" width="100" height="100" alt="dummy">
          </div>
          <div>
            サンプル
          </div>
        </div>
      </li>
      <% } -%>
    </ul>
  </div>
</div>
